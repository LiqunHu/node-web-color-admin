    "start": "npm run dev",
    "unit": "jest test/unit/specs --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "build": "node build/build.js"
  },
  "dependencies": {
    "@agametov/vueditor": "^0.4.1",
    "@xkeshi/vue-countdown": "^1.0.1",
    "bootstrap": "^4.1.3",
    "bootstrap-social": "^5.1.1",
    "bootstrap-vue": "^2.0.0-rc.11",
    "chart.js": "^2.7.2",
    "flag-icon-css": "^3.0.0",
    "ionicons": "^4.4.3",
    "moment": "^2.22.2",
    "simple-line-icons": "^2.4.1",
    "v-autocomplete": "^1.8.2",
    "mavon-editor": "^2.6.15",
    "vue": "^2.5.16",
    "axios": "^0.18.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vue-bootstrap-datetimepicker": "^5.0.0",
    "vue-chartjs": "^3.4.0",
